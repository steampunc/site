.cardTable {
	width:50%;
	display: grid;
	grid-template-columns: auto auto auto auto auto;
	gap: 10px;
	background-color: #eeeeee;
	padding: 10px;
}

.shapeLayout {
	display: flex;
  flex-wrap: wrap;
  justify-content: space-around; 
  align-items: center;
}
							
.cardTable > div {
  aspect-ratio: 0.8;
  border: 2px solid black;
  border-radius: 20px;
  background-color: rgba(255, 255, 255, 1.0);
  text-align: center;
  padding: 20px 0;
  font-size: 30px;
}
